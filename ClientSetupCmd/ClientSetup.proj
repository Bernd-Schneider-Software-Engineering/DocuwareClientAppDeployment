            <ConfigFiles Include="$(ClientSetupBaseDir64)\**\*.settings" Condition="Exists('$(ClientSetupBaseDir64)')"/>
        </ItemGroup>
        <Copy SourceFiles="@(ConfigFiles)"
              DestinationFolder="$(ConfigBackupTimeStampedFolder)\%(RecursiveDir)"
